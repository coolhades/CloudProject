<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_choose_conpany"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/diliver"
    tools:context="com.hades.mylibrary.cloud.ui.activity.ChooseConpanyActivity">

    <include layout="@layout/choosecompany_header"></include>

    <android.support.v7.widget.RecyclerView
        android:id="@+id/company_list"
        android:layout_marginTop="@dimen/padding_screen"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v7.widget.RecyclerView>
</LinearLayout>
